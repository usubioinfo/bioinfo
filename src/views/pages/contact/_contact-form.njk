<input aria-labelledby="nameFormLabel" name="name" id="formName" type="text" class="form-control kbl-form" placeholder="Full Name">
<input aria-labelledby="emailFormLabel" name="email"  id="formEmail" type="email" class="form-control kbl-form" placeholder="Email">
<input aria-labelledby="phoneFormLabel" name="phone" id="formPhone" class="form-control kbl-form"  placeholder="Phone Number">
<textarea aria-labelledby="messageFormLabel" name="message"  id="formMessage" class="form-control kbl-form" rows="15" placeholder="Say something to us!"></textarea>

<button class="btn kbl-btn-1-solid" id="send">Send Message</button>
